{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["vue", "eslint:recommended"],
  "plugins": ["vue"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": 1
  }
}